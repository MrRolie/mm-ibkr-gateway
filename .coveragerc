[run]
source = ibkr_core,api,mcp_server
omit =
    */tests/*
    */test_*.py
    */__main__.py
    */demo.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov
